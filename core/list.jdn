#include <list.h>

in std
class Node {
    private Object data;

    public Node(Object o) {
        this->data = o;
    }

    public Object getData() {
        return this->data;
    }

    public void setData(Object o) {
        this->data = o;
    }
};

in std
implement List {
    
    List() {
        this->head = nullptr;
        this->tail = nullptr;
    }
}